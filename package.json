{
  "name": "dumbcode-studio",
  "version": "1.0.0",
  "description": "Dumbcode Studio Modeler Animator and Texturer",
  "license": "MIT",
  "author": {
    "name": "DumbCode",
    "email": "placeholder@dumbcode.net",
    "url": "https://www.dumbcode.net/"
  },
  "private": false,
  "scripts": {
    "start": "run-p watch:css react-scripts:start",
    "build": "run-s build:css react-scripts:build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "cross-env TAILWIND_MODE=build NODE_ENV=production postcss src/stylesheets/tailwind.css -o src/stylesheets/index.css",
    "watch:css": "cross-env TAILWIND_MODE=watch NODE_ENV=development postcss src/stylesheets/tailwind.css -o src/stylesheets/index.css --watch",
    "react-scripts:start": "delay 5 && react-scripts start",
    "react-scripts:build": "react-scripts build"
  },
  "keywords": [
    "react",
    "tailwindcss",
    "tailwindui",
    "vscode",
    "create-react-app"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@aminere/fullik": "^1.3.3",
    "@headlessui/react": "^1.4.1",
    "@octokit/rest": "^18.12.0",
    "@savvywombat/tailwindcss-grid-areas": "^1.3.2",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "0.3.2",
    "@tailwindcss/jit": "^0.1.18",
    "@tailwindcss/typography": "^0.4.0",
    "react": "^17.0.1",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^17.0.1",
    "react-input-slider": "^6.0.0",
    "react-movable": "^2.5.3",
    "react-numeric-input": "^2.2.3",
    "react-scripts": "4.0.3",
    "react-sortablejs": "^6.0.0",
    "sortablejs": "^1.13.0",
    "tailwindcss": "^2.2.19",
    "three": "Dumb-Code/three.js",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/sortablejs": "^1.10.6",
    "@types/three": "^0.127.1",
    "@types/uuid": "^8.3.0",
    "@types/wicg-file-system-access": "^2020.9.1",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "delay-cli": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "postcss-preset-env": "^7.0.0"
  }
}
